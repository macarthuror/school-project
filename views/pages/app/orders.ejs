  <h2>Ordenes</h2>
  <div class="table-responsive">
    <table class="table table-striped table-sm">
      <thead>
        <tr>
          <th>#id</th>
          <th>Nombre del cliente</th>
        </tr>
      </thead>
      <tbody>
        <% for(const orden of ordenes){  %>
          <tr>
            <td><%= orden.id %></td>
            <td><%= orden.nombre %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
</main>

