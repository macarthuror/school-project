    </div>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.min.js" integrity="sha384-j0CNLUeiqtyaRmlzUHCPZ+Gy5fQu0dQ6eZ/xAww941Ai1SxSY+0EQqNXNE6DZiVc" crossorigin="anonymous" async></script>

  <script>
    function search() {
      return {
        searchBar: '',
        find() {
          window.location = this.searchBar ? `/app/productos?like=${this.searchBar}` : '/app/productos'
        },
        init() {
          const queryString = window.location.search;
          const urlParams = new URLSearchParams(queryString);
          this.searchBar = urlParams.get('like');
        }
      }
    }
  </script>

